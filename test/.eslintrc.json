{
  "extends": "eslint:recommended",
  "ecmaVersion": 6,
  "env": {
    "mocha": true,
    "node": true,
    "es6": true
  },
  "rules": {
    "no-console": 0,
    "comma-dangle": [
      2,
      "always-multiline"
    ],
    "no-unused-vars": [
      2,
      {
        "vars": "all",
        "args": "none"
      }
    ]
  }
}

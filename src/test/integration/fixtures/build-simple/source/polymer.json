{
  "builds": [
    {
      "name": "unbundled-uncompiled",
      "bundle": false,
      "js": {
        "compile": false,
        "minify": {
          "excludes": [
            "index.html",
            "foo.html",
            "bar.js"
          ]
        }
      },
      "css": {
        "minify": false
      },
      "html": {
        "minify": false
      }
    },
    {
      "name": "bundled-compiled",
      "bundle": true,
      "js": {
        "compile": {
          "excludes": [
            "foo.js"
          ]
        },
        "minify": {
          "excludes": [
            "bar.js"
          ]
        }
      },
      "css": {
        "minify": true
      },
      "html": {
        "minify": true
      }
    }
  ]
}

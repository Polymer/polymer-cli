{
  "npm": true,
  "moduleResolution": "node",
  "builds": [
    {
      "name": "unbundled-es",
      "bundle": false
    },
    {
      "name": "unbundled-amd",
      "bundle": false,
      "js": {
        "transformModulesToAmd": true
      }
    },
    {
      "name": "bundled-es",
      "bundle": {
        "inlineScripts": false
      }
    },
    {
      "name": "bundled-amd",
      "bundle": {
        "inlineScripts": false
      },
      "js": {
        "transformModulesToAmd": true
      }
    },
    {
      "name": "bundled-amd-minified",
      "bundle": {
        "inlineScripts": false
      },
      "js": {
        "transformModulesToAmd": true,
        "minify": true
      },
      "html": {
        "minify": true
      }
    }
  ]
}
